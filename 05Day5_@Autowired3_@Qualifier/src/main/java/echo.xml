	<!-- @어노테이션 활성화 -->
	<context:annotation-config />

	<bean id="x" class="com.test.AnotherBean">
		<property name="username" value="이순신" />
		<property name="age" value="20" />
	</bean> 
	
	<bean id="y" class="com.test.AnotherBean">
		<property name="username" value="홍길동" />
		<property name="age" value="20" />
	</bean> 
	
	<bean id="z" class="com.test.AnotherBean">
		<property name="username" value="유관순" />
		<property name="age" value="20" />
	</bean> 

	<bean id="echo" class="com.test.EchoBean"/>

</beans>
