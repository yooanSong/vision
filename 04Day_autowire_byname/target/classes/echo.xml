<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <bean  id="anotherBean" class="com.test.AnotherBean">
    <constructor-arg name="x" value="홍길동" />
    <constructor-arg name="age" value="20" />
   </bean>
  
   <bean  id="anotherBean2" class="com.test.AnotherBean">
     <property name="username" value="이순신" />
     <property name="age" value="40" />
   </bean>


	
   <bean id="echo" class="com.test.EchoBean" autowire="byName">

   </bean>




</beans>
